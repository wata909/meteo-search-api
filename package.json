{
  "name": "@agro-env/meteo-search-api",
  "version": "0.0.0",
  "main": "serverless.yml",
  "private": true,
  "devDependencies": {
    "@types/aws-lambda": "^8.10.70",
    "@types/jest": "^26.0.20",
    "@types/json2csv": "^5.0.1",
    "@types/node": "^14.14.21",
    "@types/node-fetch": "^2.5.8",
    "@types/whatwg-fetch": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "cross-env": "^7.0.3",
    "es6-promise": "^4.2.8",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "serverless": "^2.19.0",
    "serverless-offline": "^6.8.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.17",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "util": "^0.12.3",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0"
  },
  "peerDependencies": {},
  "scripts": {
    "test": "cross-env AGRO_ENV_STATIC_API_ENDPOINT='https://example.com/%s/%s/%s/%s.json' jest",
    "lint": "eslint ./src/* -c ./eslintrc.js && npm run build && npx prettier ./src/*  --write",
    "start": "npm run build && serverless offline",
    "start:lib": "npx http-server -o -c-1 docs",
    "build": "rimraf ./dist && tsc",
    "build:lib": "webpack && rimraf ./cdn && mkdir ./cdn && cp -r ./docs ./cdn/ && cp ./README.cdn.md ./cdn/README.md",
    "deploy:dev": "npm run build && serverless deploy --stage=dev",
    "deploy:v1": "npm run build && serverless deploy --stage=v1",
    "remove:dev": "npm run build && serverless remove --stage=dev",
    "remove:v1": "npm run build && serverless remove --stage=v1",
    "deploy:lib": "npm run build:lib && cd cdn && git init && git remote add origin git@github.com:agro-env/meteo-search.git && git add . && git commit -m 'Force update' && git push -f origin master"
  },
  "jest": {
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "__tests__"
    ]
  },
  "dependencies": {
    "json2csv": "^5.0.6",
    "node-fetch": "^2.6.1",
    "whatwg-fetch": "^3.6.2"
  }
}
